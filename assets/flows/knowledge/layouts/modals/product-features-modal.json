{
  "modalContainer": {
    "title": {
      "en": "Product Features Modal"
    },
    "content": {
      "firstCarousel": {
        "title": {
          "en": "Product Features"
        },
        "content": {
          "self": {
            "type": "BUTTON",
            "title": {
              "en": "Feature Type",
              "fa": "نوع ویژگی"
            },
            "icon": {
              "name": "grading"
            },
            "context": {
              "unit": {
                "firstCarousel": {
                  "title": {
                    "en": "Product Units"
                  },
                  "content": {
                    "self": {
                      "type": "BUTTON",
                      "title": {
                        "en": "Unit Type",
                        "fa": "نوع واحد"
                      },
                      "icon": {
                        "name": "grading"
                      }
                    },
                    "empty": {
                      "title": {
                        "en": "There's no unit.",
                        "fa": "هیچ واحدی وجود ندارد."
                      }
                    }
                  }
                },
                "firstInput": {
                  "type": "NUMERIC",
                  "title": {
                    "en": "Minimum Order Quantity",
                    "fa": "حداقل تعداد سفارش"
                  },
                  "options": {
                    "maxLength": 9
                  }
                },
                "secondInput": {
                  "type": "NUMERIC",
                  "title": {
                    "en": "Maximum Order Quantity",
                    "fa": "حداکثر تعداد سفارش"
                  },
                  "options": {
                    "maxLength": 9
                  }
                },
                "thirdInput": {
                  "type": "NUMERIC",
                  "title": {
                    "en": "Quantity",
                    "fa": "تعداد"
                  },
                  "options": {
                    "maxLength": 9
                  }
                }
              },
              "description": {
                "firstInput": {
                  "type": "TEXT",
                  "title": {
                    "en": "Description",
                    "fa": "توضیحات"
                  },
                  "options": {
                    "multiline": true
                  }
                }
              },
              "customized": {
                "firstInput": {
                  "type": "TEXT",
                  "title": {
                    "en": "Customized Feature Name",
                    "fa": "نام ویژگی شخصی‌سازی‌شده"
                  }
                },
                "secondInput": {
                  "type": "TEXT",
                  "title": {
                    "en": "Customized Feature Value",
                    "fa": "مقدار ویژگی شخصی‌سازی‌شده"
                  }
                }
              }
            }
          },
          "empty": {
            "state": {
              "unit": {
                "title": {
                  "en": "No unit has been remain.",
                  "fa": "هیچ واحد اندازه‌گیری باقی نمانده است."
                }
              },
              "complex": {
                "title": {
                  "en": "There's no feature.",
                  "fa": "هیچ ویژگی وجود ندارد."
                }
              }
            }
          }
        }
      },
      "secondCarousel": {
        "title": {
          "en": "Product Warehouse"
        },
        "content": {
          "self": {
            "type": "BUTTON",
            "title": {
              "en": "Warehouse Name",
              "fa": "نام انبار"
            },
            "icon": {
              "name": "grading"
            },
            "context": {
              "firstCarousel": {
                "title": {
                  "en": "Warehouses Carousel"
                },
                "content": {
                  "self": {
                    "type": "BUTTON",
                    "title": {
                      "en": "Warehouse Name",
                      "fa": "نام انبار"
                    },
                    "icon": {
                      "name": "grading"
                    }
                  },
                  "empty": {
                    "title": {
                      "en": "There's no warehouse.",
                      "fa": "هیچ انباری وجود ندارد."
                    }
                  }
                }
              },
              "firstInput": {
                "type": "NUMERIC",
                "title": {
                  "en": "Minimum Order Quantity",
                  "fa": "حداقل تعداد سفارش"
                },
                "options": {
                  "maxLength": 9
                }
              },
              "secondInput": {
                "type": "NUMERIC",
                "title": {
                  "en": "Maximum Order Quantity",
                  "fa": "حداکثر تعداد سفارش"
                },
                "options": {
                  "maxLength": 9
                }
              },
              "thirdInput": {
                "type": "NUMERIC",
                "title": {
                  "en": "Quantity",
                  "fa": "تعداد"
                },
                "options": {
                  "maxLength": 9
                }
              }
            }
          }
        }
      },
      "submitInput": {
        "type": "BUTTON",
        "prefix": {
          "en": "Append Unit",
          "fa": "اضافه کردن واحد"
        },
        "fullName": {
          "en": "Append composition of unit",
          "fa": "اضافه کردن ترکیب واحد اندازه‌گیری"
        }
      }
    }
  }
}
